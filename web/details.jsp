<%@page contentType="text/html" pageEncoding="UTF-8"%>
<%@page import="java.sql.Connection"%>
<%@page import="java.sql.DriverManager"%>
<%@page import="java.sql.*" %>
<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
        <title>JSP Page</title>
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
    </head>
    <body>
        <table class="table table-dark table-striped">
            <tr>
                <th>name</th>
                
                 <th>email</th>
                  <th>adults</th>
                   <th>children</th>
                    <th>checkin</th>
                     <th>checkout</th>
                      <th>room</th>
                      <th>confirm</th>
                      <th>reject</th>
            </tr>
        
        
        <%
            try{
        String z="select *From booking";
           Class.forName("com.mysql.jdbc.Driver");
           Connection ob=DriverManager.getConnection("jdbc:mysql://localhost:3306/resort?","root","");
          Statement St=ob.createStatement();
          ResultSet i=St.executeQuery(z);
          while(i.next()){ %>
              
              <tr>
                  <td><%=i.getString("name")%></td> 
                  <td><%=i.getString("email")%></td>
                   <td><%=i.getString("adults")%></td>
                    <td><%=i.getString("children")%></td>
                     <td><%=i.getString("checkin")%></td>
                      <td><%=i.getString("checkout")%></td>
                       <td><%=i.getString("room")%></td>
                       <td>  <a href="accept.jsp"><button type="button" class="btn btn-success">accept</button></a></td>
                        <td> <a href="delete.jsp"> <button type="button" class="btn btn-Danger">Delete</button></a></td>
              </tr>
             
            
              
         <% }}catch(Exception e){out.print(e);}%>
         </table>
    </body>
</html>
